import math
def sums(n):
	x = 0
	for w in n:
		x = x + w
	return (x)
def average(n):
	u = sums(n) // 5
	return (u)
def DS(n):
	y = average(n)
	o = 0
	for t in n:
		d = (t - y) ** 2
		o = o + d
		r = math.sqrt(o // len(n))
	return (r)

n = []
s = 0
while (s < 5):
	a = float( input("Put a value:"))
	n.append(a)
	s = s + 1

x = DS(n)
y = average(n)
z = sums(n)
print ("The Desviation Standard is:", x)
print ("The Average is:", y)
print ("The Sum of all numbers is:", z)
